application/pdf; /usr/bin/zathura %s ; copiousoutput
application/postscript ; /usr/bin/zathura %s ; copiousoutput
audio/*; /usr/bin/xdg-open %s ; copiousoutput
image/*; /usr/bin/imv %s ; copiousoutput
text/html; brave %s > /dev/null 2>&1 && sleep 5 ; test=test -n "$DISPLAY"; nametemplate=%s.html; needsterminal
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -collapse_br_tags -dump %s; nametemplate=%s.html; copiousoutput
